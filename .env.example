# REQUIRED: comma-separated hostnames you allow to be proxied (no spaces)
PROXY_ALLOWLIST=example.com,api.github.com

# Optional defaults
DEFAULT_TARGET=https://example.com
PORT=3000

# CORS: comma-separated origins allowed to call the proxy API
CORS_ORIGINS=http://localhost:3000

# Rate limit (per IP)
RATE_WINDOW_MS=60000
RATE_MAX=120
